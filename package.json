{
  "name": "shell",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "dev:all": "node ./utils/dev-server.js",
    "start": "live-server dist/shell",
    "build": "npm run build:shell && npm run build:apps",
    "build:apps": "npm run build:app1 && npm run build:header && npm run build:app2",
    "build:app1": "ng build --prod --single-bundle true --project app1 --output-hashing none --vendor-chunk false --output-path dist/shell/app1",
    "build:app2": "ng build --prod --single-bundle true --project app2 --output-hashing none --vendor-chunk false --output-path dist/shell/app2",
    "build:header": "ng build --prod --single-bundle true --project header --output-hashing none --vendor-chunk false --output-path dist/shell/header",
    "build:shell": "ng build --project shell",
    "build:apps:webpack:old-solution": "webpack",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.1.2",
    "@angular/common": "~8.1.2",
    "@angular/compiler": "~8.1.2",
    "@angular/core": "~8.1.2",
    "@angular/elements": "^8.2.0",
    "@angular/forms": "~8.1.2",
    "@angular/platform-browser": "~8.1.2",
    "@angular/platform-browser-dynamic": "~8.1.2",
    "@angular/router": "~8.1.2",
    "@ngrx/store": "^8.2.0",
    "@webcomponents/custom-elements": "^1.2.4",
    "@webcomponents/webcomponentsjs": "^2.2.10",
    "add": "^2.0.6",
    "bootstrap": "^4.3.1",
    "copy": "^0.3.2",
    "ngx-build-plus": "^8.1.3",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.2",
    "@angular/cli": "~8.1.2",
    "@angular/compiler-cli": "~8.1.2",
    "@angular/language-service": "~8.1.2",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "chalk": "^2.4.2",
    "codelyzer": "^5.0.0",
    "commander": "^3.0.0",
    "concurrently": "^4.1.1",
    "dotenv": "^8.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "live-server": "^1.2.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  }
}
